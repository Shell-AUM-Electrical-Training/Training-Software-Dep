#include <iostream>
using namespace std;

int max(int a, int b) {
    return (a > b) ? a : b;
}

int max_of_four(int a, int b, int c, int d) {
    int max_ab = max(a, b);
    int max_cd = max(c, d);
    return max(max_ab, max_cd);
}

int main() {
    int a, b, c, d;
    cin >> a >> b >> c >> d;
    int result = max_of_four(a, b, c, d);
    cout << result << endl;
    return 0;
}
